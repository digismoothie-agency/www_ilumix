<ul class="pagination-box">
  {% if paginate.previous %}
  <li class="prev">
    <a href="{{ paginate.previous.url }}" class="Previous" aria-label="Previous">
      <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><line x1="19" y1="12" x2="5" y2="12"></line><polyline points="12 19 5 12 12 5"></polyline></svg>
    </a>
  </li>
  {% endif %}
  {% for part in paginate.parts %}
  {% if part.is_link %}
    
    <li class="number">
      <a href="{{ part.url }}" title="{{ part.title }}">{% if part.title < 10 %} {{ part.title | prepend: "0" }} {% else %} {{ part.title }} {% endif %}</a>
    </li>
  
  {% else %}

  {% if part.title == paginate.current_page %}
  <li class="active">
    <a href="javascript:void(0)">{% if part.title < 10 %} {{ part.title | prepend: "0" }} {% else %} {{ part.title }} {% endif %}</a>
  </li>
  {% else %}
  <li>
    <a href="javascript:void(0)">{{ part.title }}</a>
  </li>
  {% endif %}
  {% endif %}
  {% endfor %}
  {% if paginate.next %}
  <li class="next">
    <a href="{{ paginate.next.url }}" class="Next" title="{{ paginate.next.title }}">
      <svg viewBox="0 0 24 24" width="24" height="24" stroke="currentColor" stroke-width="2" fill="none" stroke-linecap="round" stroke-linejoin="round" class="css-i6dzq1"><line x1="5" y1="12" x2="19" y2="12"></line><polyline points="12 5 19 12 12 19"></polyline></svg>
    </a>
  </li>
  {% endif %}
</ul>

<script>
  $(".theme-default-pagination .disabled a"). removeAttr("href");
  $(".theme-default-pagination li.active a"). removeAttr("href");
</script>