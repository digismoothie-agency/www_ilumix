}
.mini-cart ul.cart-item-loop::-webkit-scrollbar-thumb:hover,
.shop-sidebar ul.scrollbar::-webkit-scrollbar-thumb:hover,
.BOLD-mc-picker-mnt[data-open="up"] > .BOLD-mc-picker > .currencyList::-webkit-scrollbar-thumb:hover,
